#include <stdio.h>
#include <math.h>
int main(){
    float A,B,C,X1,X2,D,PR,PI;
    int caso;
    printf("ingrese valor de A:");
    scanf("%f", &A);
    printf("ingrese valor de B:");
    scanf("%f", &B);
    printf("ingrese valor de C:");
    scanf("%f", &C);
    
    if(A==00){
  printf("no es una ecuacion cuadratica\n");
  return 0;
}
    D=B*B - 4*A*C;
    
    if (D > 0) caso = 1;
    else if (D == 0) caso = 2;
    else caso = 3;
  
  switch (caso){
  case 1:
  X1=(-B + sqrt (D))/(2*A);
  X2=(-B - sqrt (D))/(2*A);  
  printf("dos soluciones reales:\n");
    printf("X1 = %.2f, X2 = %.2f\n", X1, X2);
    break;
case 2:
X1=-B /(2*A);
printf("una solucion real:\n");
printf("X1 = %.2f", X1);
    break;
    
    case 3:{
        PR=-B/(2*A);
        PI= sqrt (-D)/(2*A);
        printf("no hay soluciones reales.\n");
            printf("soluciones complejas:\n");
            printf("X1 = %.2f + %.2fi\n", PR, PI);
            printf("X2 = %.2f - %.2fi\n", PR, PI);
            break;
    }
    default:
            printf("Error en el c√°lculo.\n");
            break;
}
    return 0;
}
